    {
      "functions": {
        "api/**/*.php": {
          "runtime": "vercel-php@0.6.1"
        }
      },
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/api/index.php"
        },
        {
          "src": "/css/(.*)",
          "dest": "/css/$1"
        },
        {
          "src": "/js/(.*)",
          "dest": "/js/$1"
        }
      ]
    }
